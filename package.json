{
  "name": "reactflow-family-tree",
  "private": false,
  "version": "1.0.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/harrykhh/react-flow-family-tree"
  },
  "main": "./dist/reactflow-family-tree.umd.js",
  "module": "./dist/reactflow-family-tree.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/reactflow-family-tree.es.js",
      "require": "./dist/reactflow-family-tree.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": ">20.0.0"
  },
  "scripts": {
    "commit": "cz",
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@storybook/addon-a11y": "^9.1.15",
    "@storybook/addon-links": "^9.1.15",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-themes": "^9.1.15",
    "@storybook/react-vite": "^9.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.20",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.18.12",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.9",
    "babel-loader": "^9.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^9.1.15",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "json": "^11.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.9",
    "@storybook/addon-docs": "^9.1.15"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
